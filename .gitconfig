[user]
	email = ravencarlostrodrigo@gmail.com
	name = Raven Rodrigo

[color]
 ui = auto

[color "branch"]
 current = yellow reverse
 local = yellow
 remote = green

[color "diff"]
 meta = yellow bold
 frag = magenta bold
 old = red bold
 new = green bold
 whitespace = red reverse

[color "status"]
 added = green bold
 changed = yellow bold
 untracked = red bold
[init]
 defaultBranch = master
[filter "tabspace"]
 smudge = unexpand --tabs=1 --first-only
 clean = expand --tabs=1 --initial
